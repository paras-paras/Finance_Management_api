
@{
    ViewBag.Title = "HeadLogin";
}
    <h2>Head Login</h2><br/>
<div class="container">
    <div class="row">
        
        <form method="post" action="head_logincheck">
           <div class="col-md-12">
                <div class="form-group-lg">
                    <label>Enter Name </label>
                    <input type="text" name="head_Name" id="head_Name" placeholder="Enter Name " class="form-control" />

                </div>

                <div class="form-group-lg">
                    <label>Enter Password </label>
                    <input type="password" name="head_Password" id="head_Password" placeholder="Enter Password " class="form-control" />

                </div>

                <input type="submit" value="Login " class="btn btn-primary" />
            </div>
        </form>
    </div>

</div>
