@model IEnumerable<FinanceManagementApi.Models.ServiceRecord>

@{
    ViewBag.Title = "userServices";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="text-lg-center">View All Services </h1>
        </div>
        @foreach (var item in Model)
        {
              <div class="col-md-6">
                  <h1>  @Html.DisplayFor(modelItem => item.ServiceName)</h1>
                  <p>    @Html.DisplayFor(modelItem => item.Features)</p>
              </div>

        }

        </div>

</div>

